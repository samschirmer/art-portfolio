<div id="about_container">
	<div class="row">
		<div class="col-xs-12 col-lg-8">
			<h1><%= @page.header %></h1>
			<p><%= @page.content %></p>
		</div>
	<div class="col-xs-12 col-lg-4">
		<div id="instagram_feed" class="instagram_feed"></div>
	</div>
</div>
</div>
<script type="text/javascript" src="https://cdn.shurimages.com/js/libs/jquery.min.js"></script>
<script type="text/javascript" src="/javascript/jquery.instagramFeed.min.js"></script>
<script type="text/javascript">
(function($){
	$(window).on('load', function(){				
		$.instagramFeed({
			'username': 'katelinhull',
			'container': "#instagram_feed",
			'display_profile': false,
			'display_biography': false,
			'display_gallery': true,
			'get_raw_json': false,
			'callback': null,
			'styling': true,
			'items': 9,
			'items_per_row': 3,
			'margin': 1
		});
	});
})(jQuery);
</script>
