<div id="form_container">
	<form action="#" method="post">
		<div class="form-group">
			<label>Name</label>
			<input class="form-control" required="required" name="name" />
		</div>
		<div class="form-group">
			<label>Email</label>
			<input class="form-control" type="email" required="required" name="email" />
		</div>
		<div class="form-group">
			<label>Message</label>
			<textarea class="form-control" required="required" name="message"></textarea>
		</div>
		<button type="submit">Submit</button>
	</form>
</div>

